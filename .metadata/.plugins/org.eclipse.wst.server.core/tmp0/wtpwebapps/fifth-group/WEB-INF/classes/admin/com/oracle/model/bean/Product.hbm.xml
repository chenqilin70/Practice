<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="admin.com.oracle.model.bean.Product" table="product">
			<id name="id">
				<generator class="productid"></generator>
			</id>
			<property name="name"></property>
			<property name="brand"></property>
			<property name="seris"></property>
			<property name="type"></property>
			<property name="description"></property>
			<property name="basicprice"></property>
			<property name="typeid"></property>
			<property name="image"></property>
	</class>
</hibernate-mapping>