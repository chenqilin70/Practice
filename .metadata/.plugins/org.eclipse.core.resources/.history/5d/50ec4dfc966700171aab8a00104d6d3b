select count(*) from orders where month(TIME)='01' && dayofmonth(TIME)='01'

select DATE_FORMAT(TIME,'%Y-%m-%d'),count(*) from orders  where year(TIME)='2017' group by dayofyear(TIME)

select distinct dayofyear(TIME) from orders order by dayofyear(TIME)

desc orders

select * from product

insert into orderitems